extends base.pug

block scripts 
   script(src="/static/index.js") 

block style 
    style
      include ../static/style.css
      include ../static/styleContact.css

block content 
      div.container 
            h1 Contact Us
            form.myForm(action='/contact' method='POST')
                input.myInput(type='text' ,='' name='name' placeholder='Enter Your Name')
                input.myInput(type='phone' ,='' name='phone' placeholder='Enter Your Phone No:')
                input.myInput(type='email' ,='' name='email' placeholder='Enter Your Email')
                input.myInput(type='text' ,='' name='address' placeholder='Enter Your Address')
                input.myInput(type='text' ,='' name='desc' placeholder='Enter Your Concern')
                button(class="btn")
                  |Submit
      
